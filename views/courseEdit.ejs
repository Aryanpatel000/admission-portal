<h1>Course Edit</h1>
<div class="container">
    <form action="/courseUpdate/<%= d._id %>" method="post" enctype="multipart/form-data">
        <div class="mb-3">
         <label>Name:</label>
         <input type="text" name="name" placeholder="name" class="form-control"value="<%= d._id %>">
        </div>

        <div class="mb-3">
          <label>Email:</label>
          <input type="email" name="email"placeholder="name" class="form-control"value="<%= d.email %>">
         </div>

         <div class="mb-3">
          <label>Phone:</label>
          <input type="number" name="phone" class="form-control" value="<%= d.phone %>">
         </div>

         <div class="mb-3">
          <label>City:</label>
          <input type="text" name="city" class="form-control"value="<%= d.city %>">
         </div>

         <div class="mb-3">
          <label>Address:</label>
          <input type="text" name="address" class="form-control" value="<%= d.address %>">
         </div>

         <div class="mb-3">
          <label>Course:</label>
          <input type="text" name="course" value="mca" class="form-control" value="<%= d.course %>">
         </div>
         <button type="submit" class="btn btn-info">submit</button>
      </form>
</div>

<%- include('./footer.ejs')  %>