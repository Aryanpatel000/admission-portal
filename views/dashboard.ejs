<%- include('./header.ejs') %>

<div class="carousel slide dashpage" data-bs-ride="carousel" id="a">
  <div class="carousel-indicators">
    <button type="button" data-bs-target="#a" data-bs-slide-to="0" class="active" aria-current="true"></button>
    <button type="button" data-bs-target="#a" data-bs-slide-to="1" class=""></button>
    <button type="button" data-bs-target="#a" data-bs-slide-to="2" class=""></button>
    <button type="button" data-bs-target="#a" data-bs-slide-to="3" class=""></button>
    <button type="button" data-bs-target="#a" data-bs-slide-to="4" class=""></button>
  </div>


  <div class="carousel-inner">
    <div class="carousel-item" data-bs-interval="4000">
      <img src="image/mits1.jpg" class="w-100 " height="650px" alt="...">
    </div>
    <div class="carousel-item" data-bs-interval="2000">
      <img src="image/mits8.jpg" class="w-100 " height="650px" alt="...">
    </div>
    <div class="carousel-item active" data-bs-interval="2000">
      <img src="image/mits6.jpg" class="w-100 " height="650px" alt="...">
    </div>
    <div class="carousel-item" data-bs-interval="2000">
      <img src="image/mits.jpg" class="w-100 " height="650px" alt="...">
    </div>
    <div class="carousel-item" data-bs-interval="2000">
      <img src="image/mits5.jpg" class="w-100 " height="650px" alt="...">
    </div>
  </div>

  <button class="carousel-control-prev" type="button" data-bs-target="#a" data-bs-slide="prev">
    <span class="carousel-control-prev-icon"></span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#a" data-bs-slide="next">
    <span class="carousel-control-next-icon"></span>
  </button>
</div>




<div class="container text-black mb-5 text-center background-color:a2a2e0ad">
  <h1 class="course_heading mt-5">Hello
  </h1>
  <h2 class="course_heading my-3">Register Your Course</h2>
  <p class="course_para text-center mb-5">Course Registration means the official recording of a student's name to
    study a course or courses.</p>
  <div class="row">

    <div class="col-md-4 mb-5">
      <div class="card card1">
        <img class="card-img-top" height="200px" width="300px"
          src="image/card_image1.jpg" alt="Card image cap">

        <div class="card-body">
          <h5 class="card-title ">B.Tech</h5>
          <p class="card-text">B.Tech is a professional undergraduate engineering degree</p>
          <% if(b){ %>
             <a href="/courseDisplay">Display</a>
          <% }else { %>
            <a href="btn" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#btech">Register</a>
          <%}%>
          

        </div>
      </div>
    </div>

    <div class="col-md-4 mb-4">
      <div class="card card2">
        <img class="card-img-top " height="200px!important" width="300px!important"
          src="image/card_image2.jpg" alt="Card image cap">

        <div class="card-body">
          <h5 class="card-title ">BCA</h5>
          <p class="card-text">BCA is a three-year Bachelor's Degree in a Computer Application </p>
          <% if(b){ %>
            <a href="/courseDisplay">Display</a>
         <% }else { %>
          <a href="btn" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#BCA">Register</a>
         <%}%>
         


        </div>
      </div>
    </div>

    <div class="col-md-4 mb-4">
      <div class="card card3">
        <img class="card-img-top " height="200px" width="300px"
          src="image/card_image3.jpg" alt="Card image cap">

        <div class="card-body">
          <h5 class="card-title ">MCA</h5>
          <p class="card-text">MCA is a professional post-graduation degree in computer science</p>
          <% if(b){ %>
            <a href="/courseDisplay">Display</a>
         <% }else { %>
          <a href="btn" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#mca">Register</a>
         <%}%>
         

        </div>
      </div>
    </div>

  </div>
</div>



<!-- btech modal -->
  <div class="modal fade" id="btech" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">B Tech</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <form action="/course-insert" method="post">
            <div class="mb-3">
             <label>Name:</label>
             <input type="text" name="name" placeholder="name" class="form-control">
            </div>

            <div class="mb-3">
              <label>Email:</label>
              <input type="email" name="email"placeholder="name" class="form-control">
             </div>

             <div class="mb-3">
              <label>Phone:</label>
              <input type="number" name="phone" class="form-control">
             </div>

             <div class="mb-3">
              <label>City:</label>
              <input type="text" name="city" class="form-control">
             </div>

             <div class="mb-3">
              <label>Address:</label>
              <input type="text" name="address" class="form-control">
             </div>

             <div class="mb-3">
              <label>Course:</label>
              <input type="text" name="course" value="btech" class="form-control">
             </div>
             <button type="submit" class="btn btn-info">submit</button>
          </form>
        </div>
        
      </div>
    </div>
  </div>




